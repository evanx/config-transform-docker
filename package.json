{
  "name": "config-transform-docker",
  "version": "0.1.0",
  "description": "A service to select element from a document loaded by PhantomJS.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --cache src",
    "development": "NODE_ENV=development node --harmony src/index.js",
    "test": "NODE_ENV=test node --harmony src/index.js",
    "tmp": "NODE_ENV=development node --harmony tmp/index.js",
    "start": "node --harmony src/index.js",
    "pm2start": "pm2 start src/index.js --node-args='--harmony' --name `basename $PWD`"
  },
  "author": "Evan Summers",
  "license": "ISC",
  "dependencies": {
    "@f/require-content": "^1.0.2",
    "bluebird": "^3.4.6",
    "get-stdin": "^5.0.1",
    "lodash": "^4.17.14",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanx/config-transform-docker.git"
  },
  "bugs": {
    "url": "https://github.com/evanx/config-transform-docker/issues"
  },
  "homepage": "https://github.com/evanx/config-transform-docker#readme"
}
